<div id="content-header" class="mini hidden-xs">
  <h1>All TDNPs</h1>
</div>

<div id="breadcrumb">
  <%= make_breadcrumb_link('dashboard', '/', 'Dashboard') %>
  <%= make_breadcrumb_link('users', customer_show_tdnps_path, 'All TDNPs', 'current') %>
</div>

<%= render partial: 'layouts/messages' %>

<div class="container-fluid row">
  <div class="col-xs-12">
    <div class="widget-box">
      <div class="widget-title">
        <span class="icon"><%= fa_icon 'users' %></span>
        <h5>All TDNP Customers</h5>
      </div>
      <div class="widget-content nopadding table-responsive">
        <table class="table table-striped table-bordered table-hover mycustomclass" id="table-range">
          <thead>
            <tr>
              <th>IP Address</th>
              <th class="hidden-xs">Account Number</th>
              <th>Customer Name</th>
              <th>Home Phone</th>
              <th>TDNP</th>
            </tr>
          </thead><!-- end thead -->

          <tbody>
            <% @results.order(:id).each do |customer| %>
              <%= make_range_table_entry(customer.ip_address) %>
            <% end %>
          </tbody><!-- end tbody -->
        </table><!-- end table -->
      </div><!-- end widget-content -->
    </div><!-- end widget-box-->
  </div><!-- end col-xs-12 -->
</div><!-- end container-fluid -->
